{
	"name": "@sveltejs/kit",
	"version": "1.0.0-next.134",
	"type": "module",
	"dependencies": {
		"@sveltejs/vite-plugin-svelte": "^1.0.0-next.14",
		"cheap-watch": "workspace:^1.0.3",
		"sade": "workspace:^1.7.4",
		"vite": "^2.4.3"
	},
	"devDependencies": {
		"@rollup/plugin-replace": "^3.0.0",
		"@types/amphtml-validator": "^1.0.1",
		"@types/cookie": "^0.4.1",
		"@types/globrex": "^0.1.1",
		"@types/marked": "^2.0.4",
		"@types/mime": "^2.0.3",
		"@types/node": "^16.4.1",
		"@types/rimraf": "^3.0.1",
		"@types/sade": "^1.7.3",
		"amphtml-validator": "^1.0.35",
		"cookie": "^0.4.1",
		"devalue": "^2.0.1",
		"eslint": "^7.31.0",
		"globrex": "^0.1.2",
		"kleur": "^4.1.4",
		"locate-character": "^2.0.5",
		"marked": "^2.1.3",
		"node-fetch": "^3.0.0-beta.10",
		"port-authority": "^1.1.2",
		"rimraf": "^3.0.2",
		"rollup": "^2.53.3",
		"selfsigned": "^1.10.11",
		"sirv": "^1.0.12",
		"svelte": "^3.40.1",
		"svelte-check": "^2.2.3",
		"svelte2tsx": "~0.4.2",
		"tiny-glob": "^0.2.9",
		"typescript": "^4.3.5",
		"uvu": "^0.5.1"
	},
	"peerDependencies": {
		"svelte": "^3.40.1"
	},
	"bin": {
		"svelte-kit": "svelte-kit.js"
	},
	"files": [
		"assets",
		"dist",
		"types",
		"svelte-kit.js"
	],
	"scripts": {
		"build": "rollup -c",
		"dev": "rollup -cw",
		"lint": "eslint --ignore-path .gitignore --ignore-pattern \"src/core/make_package/test/**\" \"src/**/*.{ts,mjs,js,svelte}\" && npm run check-format",
		"check": "tsc && svelte-check --ignore \"src/core/make_package/test\"",
		"format": "prettier --write . --config ../../.prettierrc --ignore-path .gitignore",
		"check-format": "prettier --check . --config ../../.prettierrc --ignore-path .gitignore",
		"prepublishOnly": "npm run build",
		"test": "npm run test:unit && npm run test:integration",
		"test:unit": "uvu src \"(spec\\.js|test[\\\\/]index\\.js)\"",
		"test:integration": "uvu test test.js"
	},
	"exports": {
		"./package.json": "./package.json",
		"./ssr": {
			"import": "./dist/ssr.js"
		},
		"./node": {
			"import": "./dist/node.js"
		},
		"./install-fetch": {
			"import": "./dist/install-fetch.js"
		},
		"./adapter-utils": {
			"import": "./dist/adapter-utils.js"
		},
		"./types": "./types/index.d.ts"
	},
	"types": "types/index.d.ts",
	"engines": {
		"node": "^12.20 || >=14.13"
	}
}